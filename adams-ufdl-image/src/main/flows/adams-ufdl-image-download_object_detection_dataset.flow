adams.flow.control.Flow -annotation "Let\\\'s the user choose an object detection dataset to download (as zip or tar.gz).\\nUses host/user/password from the global UFDL Preferences." -flow-execution-listener adams.flow.execution.NullListener -flow-restart-manager adams.flow.control.flowrestart.NullManager
 adams.flow.standalone.UFDLConnection
 adams.flow.source.Start
 adams.flow.control.Trigger -name "prompt user"
  adams.flow.standalone.SetVariable -name "restore file" -var-name restore -var-value @{flow_filename_long}.props -value-type FILE_FORWARD_SLASHES -expand-value true
  adams.flow.source.EnterManyValues -stop-if-canceled true -value "adams.flow.source.valuedefinition.ObjectDetectionDatasetList -name dataset -display \"Dataset to download\"" -value "adams.flow.source.valuedefinition.DefaultValueDefinition -name format_params -display \"Format options\" -help \"the output parameters to pass on to wai.annotations\"" -value "adams.flow.source.valuedefinition.DefaultValueDefinition -name output -display \"Output file (zip or tar.gz)\" -type FILE_ABSOLUTE -use-forward-slashes true" -output-type MAP -restoration-enabled true -restoration-file @{restore}
  adams.flow.transformer.MapToVariables
  adams.flow.control.Trigger -name "get dataset ID"
   adams.flow.source.Variable -var-name dataset -conversion adams.data.conversion.StringToString
   adams.flow.transformer.Convert -conversion adams.data.conversion.UFDLExtractListItemPK
   adams.flow.transformer.SetVariable -var-name dataset_pk
 adams.flow.control.Trigger -name "download dataset"
  adams.flow.source.Variable -var-name dataset_pk -conversion adams.data.conversion.StringToInt
  adams.flow.sink.UFDLSink -action "adams.flow.sink.ufdl.DownloadObjectDetectionDataset -parameters @{format_params} -output-file @{output}"