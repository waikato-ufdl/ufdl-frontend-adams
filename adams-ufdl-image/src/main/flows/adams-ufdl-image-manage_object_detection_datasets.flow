     adams.flow.transformer.SetVariable -var-name success
     adams.flow.control.Trigger -name info
      adams.flow.source.CombineVariables -expression "copied dataset @{dataset_pk} to \\\"@{new_name}\\\": @{success}" -conversion adams.data.conversion.StringToString
